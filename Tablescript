function doGet() {
  // The ID of the Google Sheet
  var sheetId = '';

  // Open the sheet by ID
  var sheet = SpreadsheetApp.openById(sheetId).getSheetByName('UPSTOX');
  
  // Get the range of data you want to convert to HTML
  var range = sheet.getRange('A1:D20'); // Change this range as needed
  var values = range.getValues();
  
  // Convert the range data to an HTML table
  var html = '<html><body></br>TRADE NOW FROM BROKER(UPSTOX-DHAN)<table id='optionb'><thead><tr><th>CALL</th><th>PUT</th><th>CALL</th><th>PUT</th></tr></thead>';
  
  for (var i = 0; i < values.length; i++) {
    html += '<tr>';
    for (var j = 0; j < values[i].length; j++) {
      html += '<td>' + values[i][j] + '</td>';
    }
    html += '</tr>';
  }
  
  html += '</table></body></html>';
  
  // Return the HTML content
  return HtmlService.createHtmlOutput(html);
}




